{
  "compilerOptions": {
    "target": "es5",
    "module": "commonjs",
    "moduleResolution": "node",
    "experimentalDecorators": true,
    "emitDecoratorMetadata": true,
    "allowSyntheticDefaultImports": true,
    "removeComments": false,
    "sourceMap": true,
    "inlineSourceMap": true,
    "strictNullChecks": false,
    "lib": [
      "es2015"
    ],
    "types": [
      "node"
    ]
  },
  "compileOnSave": false
}
